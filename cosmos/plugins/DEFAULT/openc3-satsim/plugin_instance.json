{
  "name": "openc3-satsim-0.2.0.gem__20240422132025",
  "variables": {
    "cgs_iface_port": "9999",
    "cgs_iface_name": "CGS_TCP",
    "log_retain_time": "172800",
    "reduced_log_retain_time": "2592000",
    "tlm_log_cycle_time": "300",
    "tlm_buffer_depth": "128"
  },
  "plugin_txt_lines": [
    "VARIABLE cgs_iface_port 9999",
    "VARIABLE cgs_iface_name CGS_TCP",
    "",
    "VARIABLE log_retain_time 172800",
    "VARIABLE reduced_log_retain_time 2592000",
    "VARIABLE tlm_log_cycle_time 300",
    "VARIABLE tlm_buffer_depth 128",
    "",
    "NEEDS_DEPENDENCIES",
    "",
    "TARGET CGS CGS",
    "  LOG_RETAIN_TIME <%= log_retain_time %>",
    "  REDUCED_LOG_RETAIN_TIME <%= reduced_log_retain_time %>",
    "  TLM_LOG_CYCLE_TIME <%= tlm_log_cycle_time %>",
    "  TLM_BUFFER_DEPTH <%= tlm_buffer_depth %>",
    "",
    "<% (1..6).each do |i| %>",
    "  TARGET GALAXY<%= i %> \"GALAXY-<%= i %>\"",
    "    LOG_RETAIN_TIME <%= log_retain_time %>",
    "    REDUCED_LOG_RETAIN_TIME <%= reduced_log_retain_time %>",
    "    TLM_LOG_CYCLE_TIME <%= tlm_log_cycle_time %>",
    "    TLM_BUFFER_DEPTH <%= tlm_buffer_depth %>",
    "<% end %>",
    "",
    "<% (1..3).each do |i| %>",
    "  TARGET CLUSTER<%= i %> \"CLUSTER-<%= i %>\"",
    "    LOG_RETAIN_TIME <%= log_retain_time %>",
    "    REDUCED_LOG_RETAIN_TIME <%= reduced_log_retain_time %>",
    "    TLM_LOG_CYCLE_TIME <%= tlm_log_cycle_time %>",
    "    TLM_BUFFER_DEPTH <%= tlm_buffer_depth %>",
    "<% end %>",
    "",
    "<% (1..5).each do |i| %>",
    "  TARGET GOES<%= i %> \"GOES-<%= i %>\"",
    "    LOG_RETAIN_TIME <%= log_retain_time %>",
    "    REDUCED_LOG_RETAIN_TIME <%= reduced_log_retain_time %>",
    "    TLM_LOG_CYCLE_TIME <%= tlm_log_cycle_time %>",
    "    TLM_BUFFER_DEPTH <%= tlm_buffer_depth %>",
    "<% end %>",
    "",
    "INTERFACE <%= cgs_iface_name %> tcpip_server_interface.rb <%= cgs_iface_port %> <%= cgs_iface_port %> 1.0 nil LENGTH 0 32 0 1 LITTLE_ENDIAN 4",
    "  <% (1..6).each do |i| %>",
    "    MAP_TARGET \"GALAXY-<%= i %>\"",
    "  <% end %>",
    "  <% (1..3).each do |i| %>",
    "    MAP_TARGET \"CLUSTER-<%= i %>\"",
    "  <% end %>",
    "  <% (1..5).each do |i| %>",
    "    MAP_TARGET \"GOES-<%= i %>\"",
    "  <% end %>",
    "  MAP_TARGET CGS",
    "  OPTION LISTEN_ADDRESS 0.0.0.0",
    "",
    "WIDGET MAPVIEWER"
  ],
  "needs_dependencies": true,
  "updated_at": 1713792025193358752
}